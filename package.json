{
  "name": "qiniu-loader",
  "version": "0.0.1",
  "description": "Loaders file as qiniu cdn URL",
  "scripts": {
    "start": "npm run build -- -w",
    "build": "cross-env NODE_ENV=production babel src -d dist --ignore 'src/**/*.test.js' --copy-files",
    "clean": "del-cli dist",
    "lint": "eslint --cache src test",
    "lint-staged": "lint-staged",
    "prepare": "npm run build",
    "prebuild": "npm run clean",
    "release": "standard-version",
    "security": "nsp check",
    "webpack-defaults": "webpack-defaults"
  },
  "engines": {
    "node": ">= 8.0.0"
  },
  "main": "dist/cjs.js",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/moosoul/qiniu-loader.git"
  },
  "keywords": [
    "webpack",
    "qiniu",
    "loader"
  ],
  "author": "moosoul <m.moosoul@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/moosoul/qiniu-loader/issues"
  },
  "homepage": "https://github.com/moosoul/qiniu-loader#readme",
  "devDependencies": {
    "babel-cli": "^6.0.0",
    "babel-jest": "^21.0.0",
    "babel-plugin-transform-object-rest-spread": "^6.0.0",
    "babel-polyfill": "^6.0.0",
    "babel-preset-env": "^1.0.0",
    "cross-env": "^5.0.0",
    "del": "^3.0.0",
    "del-cli": "^1.0.0",
    "eslint": "^4.0.0",
    "eslint-config-webpack": "^1.0.0",
    "eslint-plugin-import": "^2.0.0",
    "jest": "^21.0.0",
    "lint-staged": "^5.0.0",
    "memory-fs": "^0.4.0",
    "nsp": "^2.0.0",
    "pre-commit": "^1.0.0",
    "standard-version": "^4.0.0",
    "webpack": "^3.0.0",
    "webpack-defaults": "^1.6.0"
  },
  "dependencies": {
    "loader-utils": "^1.1.0",
    "qiniu": "^7.1.3"
  },
  "peerDependencies": {
    "webpack": "^2.0.0 || ^3.0.0 || ^4.0.0"
  },
  "pre-commit": "lint-staged",
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  }
}
